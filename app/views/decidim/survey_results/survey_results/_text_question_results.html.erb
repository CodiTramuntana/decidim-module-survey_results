<table>
  <thead>
    <tr>
      <th></th>
      <th><%= t(".table.percentage") %></th>
      <th><%= t(".table.quantity") %></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= t(".table.answered") %></td>
      <td><%= results.total_answers_answered_percentage %></td>
      <td><%= results.total_countable_answers_count %></td>
    </tr>
    <tr>
      <td><%= t(".table.not_answered") %></td>
      <td><%= results.total_answers_not_answered_percentage %></td>
      <td><%= full_questionnaire.total_participants - results.total_countable_answers_count %></td>
    </tr>
  </tbody>
</table>
